{
  "compilerOptions": {
    "noImplicitAny": false,
    "removeComments": true,
    "preserveConstEnums": true,
    "target": "ES5",
    "lib": ["dom", "es6"],
    "module": "system",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "sourceMap": true,
    "baseUrl": "src/main/webapp/assets"
  },
  "exclude": [
    "node_modules",
    "target"
  ]
}